
function init() {
    canvas = document.getElementById("canvasId");
    body = document.getElementById("body");
    if (canvas.width < body.offsetWidth) {
        canvas.width = body.offsetWidth;
    }

    if (canvas.height < body.offsetHeight) {
        canvas.height = body.offsetHeight;
    }

